nd I do not wish to appear as though I had.\r","standoffProperties":[{"id":"936b8464-1cc1-4284-a9ab-90bf16363ca7","type":"codex/entity-reference","start":0,"end":1,"value":"054323f9-18b2-46c4-9d8a-56d26ecb41b0","text":"Y","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"9bb15d7d-2a0a-4ce2-b908-b98a8870031d","type":"codex/entity-reference","start":13,"end":13,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","text":"","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"eeed324d-a7c0-4078-ba41-f3ccc136d909","type":"codex/entity-reference","start":20,"end":28,"value":"ddc8033b-8afe-44e9-b845-313d6433e7ba","text":"Balducci","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"41643018-970b-4a5e-b27f-150b50c79f34","type":"codex/entity-reference","start":30,"end":31,"value":"ddc8033b-8afe-44e9-b845-313d6433e7ba","text":"h","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"88425506-0c51-412f-8fd9-acc49911ce2f","type":"codex/trait-reference","start":56,"end":59,"value":"c6e6f96a-0c8c-48c8-994d-01f969d43079","text":"lim","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"d60808d4-3646-42a9-a5ac-a53cd50c464e","type":"codex/entity-reference","start":95,"end":98,"value":"a01823db-117c-40e3-9764-c6033febb236","text":"wha","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"c424a51f-aa3f-4eb9-85fc-3d4274bfa6c4","type":"codex/entity-reference","start":100,"end":101,"value":"ddc8033b-8afe-44e9-b845-313d6433e7ba","text":"h","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"8e638985-c068-4cee-aa54-fba82ab2e0b1","type":"codex/entity-reference","start":170,"end":172,"value":"ddc8033b-8afe-44e9-b845-313d6433e7ba","text":"hi","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},{"id":"7c1a9105-abcf-4641-8ba0-1f12786a4a8f","type":"codex/entity-reference","start":189,"end":190,"value":"a01823db-117c-40e3-9764-c6033febb236","text":"i","metadata":{"offsetY":0},"plugin":null,"isDeleted":false},