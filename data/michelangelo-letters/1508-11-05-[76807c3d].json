 November.","standoffProperties":[{"id":"31e1b105-41d5-485a-b2f3-872943210f5b","type":"codex/entity-reference","start":0,"end":0,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","zeroPoint":false,"metadata":[]},{"id":"d4fb7d5a-01c6-4618-9215-242e9c685178","type":"codex/entity-reference","start":20,"end":22,"value":"21671569-3eea-412b-85cc-08f659f58019","zeroPoint":false,"metadata":[]},{"id":"265757e5-b0ed-4e8a-89d1-d29c9adb6138","type":"codex/entity-reference","start":51,"end":53,"value":"66077dcf-3674-48a2-be09-76b3553dae5d","zeroPoint":false,"metadata":[]},{"id":"e4e91b5d-3328-4a80-b225-ddc647a58f73","type":"codex/entity-reference","start":73,"end":80,"value":"671ee7e7-ae1b-4a5a-9591-b40203d5108f","zeroPoint":false,"metadata":[]},{"id":"23714fe5-78c8-452d-8c41-c3070a402f93","type":"codex/entity-reference","start":181,"end":182,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","zeroPoint":false,"metadata":[]},{"id":"d7d0eb36-28dc-4b33-8ded-879c58fab726","type":"codex/entity-reference","start":185,"end":185,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","zeroPoint":false,"metadata":[]},{"id":"5429938d-5976-48b3-a1bf-b5bd83ef674f","type":"codex/entity-reference","start":199,"end":201,"value":"66077dcf-3674-48a2-be09-76b3553dae5d","zeroPoint":false,"metadata":[]},{"id":"d44c125f-2ecf-4a40-ae52-f5e60b6d3961","type":"codex/entity-reference","start":316,"end":317,"value":"66077dcf-3674-48a2-be09-76b3553dae5d","zeroPoint":false,"metadata":[]},{"id":"7a3c628f-43b6-4914-911f-f327ed4c86e2","type":"codex/entity-reference","start":363,"end":365,"value":"21671569-3eea-412b-85cc-08f659f58019","zeroPoint":false,"metadata":[]},{"id":"5eb6bb0b-39af-40e1-bd60-cb01ada7e3de","type":"codex/entity-reference","start":395,"end":396,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","zeroPoint":false,"metadata":[]},{"id":"ce07f334-24e9-48b2-82f8-cfd270133f4f","type":"codex/entity-reference","start":451,"end":451,"value":"add92e01-cefe-4f13-94dd-f00eb82155b0","zeroPoint":false,"metadata":[]},{"id":"