f its formidable unity.\r","standoffProperties":[],"blockProperties":[],"children":[],"metadata":{},"relation":{}},{"id":"6cbeffe4-7ccd-46bd-8e3c-121acefe6d03","type":"standoff-editor-block","text":"\r","standoffProperties":[],"blockProperties":[],"children":[],"metadata":{},"relation":{}}],"metadata":{"focus":{"blockId":"6bf51124-a73a-4eb2-a68d-e9d3a2499f37","caret":12}}}